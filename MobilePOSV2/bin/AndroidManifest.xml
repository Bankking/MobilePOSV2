<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.mobileposv2"
    android:versionCode="1"
    android:versionName="1.0" >

    <uses-sdk
        android:minSdkVersion="11"
        android:targetSdkVersion="18" />

    <application
        android:allowBackup="true"
        android:icon="@drawable/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name="ku.mobilepos.activity.MainActivity"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name="ku.mobilepos.activity.InventoryAddMoreItemActivity" >
        </activity>
        <activity android:name="ku.mobilepos.activity.SaleSelectItemActivity" >
        </activity>
        <activity android:name="ku.mobilepos.activity.CustomerAddNewCustomerActivity" >
        </activity>
    </application>

</manifest>
